{
    "name":  "@sage-box/icons",
    "version":  "0.1.0",
    "description":  "SVG Icon component for SageBox - A flexible, accessible icon system",
    "main":  "dist/index.cjs.js",
    "module":  "dist/index.js",
    "types":  "dist/types/index.d.ts",
    "bin":  {
                "sagebox":  "./bin/cli.js"
            },
    "collection":  "dist/collection/collection-manifest.json",
    "collection:main":  "dist/collection/index.js",
    "unpkg":  "dist/sagebox-icons/sagebox-icons.esm.js",
    "exports":  {
                    ".":  {
                              "types":  "./dist/types/index.d.ts",
                              "import":  "./dist/index.js",
                              "require":  "./dist/index.cjs.js"
                          },
                    "./loader":  {
                                     "types":  "./loader/index.d.ts",
                                     "import":  "./loader/index.js",
                                     "require":  "./loader/index.cjs"
                                 },
                    "./cli":  {
                                  "require":  "./cli/index.js"
                              },
                    "./cli/plugins/vite":  {
                                               "require":  "./cli/plugins/vite.js"
                                           }
                },
    "repository":  {
                       "type":  "git",
                       "url":  "https://github.com/Adravilag/sagebox.git",
                       "directory":  "packages/icons"
                   },
    "bugs":  {
                 "url":  "https://github.com/Adravilag/sagebox/issues"
             },
    "homepage":  "https://github.com/Adravilag/sagebox/tree/main/packages/icons#readme",
    "files":  [
                  "dist/",
                  "loader/",
                  "bin/",
                  "cli/"
              ],
    "sideEffects":  [
                        "dist/sagebox-icons/*.js",
                        "dist/esm/*.js",
                        "loader/index.js"
                    ],
    "scripts":  {
                    "build":  "stencil build",
                    "build:watch":  "stencil build --watch",
                    "test":  "stencil test --spec",
                    "test:watch":  "stencil test --spec --watchAll",
                    "test:coverage":  "stencil test --spec --coverage",
                    "generate-icons":  "node scripts/generate-icons.js",
                    "add-icon":  "node scripts/generate-icons.js add"
                },
    "devDependencies":  {
                            "@stencil/core":  "^4.27.1",
                            "@types/jest":  "^30.0.0",
                            "jest":  "^30.2.0",
                            "jest-cli":  "^30.2.0",
                            "typescript":  "^5.9.3"
                        },
    "dependencies":  {
                         "commander":  "^12.1.0"
                     },
    "keywords":  [
                     "sagebox",
                     "icons",
                     "svg-icons",
                     "web-components",
                     "stencil",
                     "icon-component",
                     "custom-elements"
                 ],
    "author":  "adravilag",
    "license":  "MIT",
    "publishConfig":  {
                          "access":  "public"
                      }
}
