{
  "name": "saged-ui",
  "version": "1.1.0",
  "description": "SagedUI - Modern Web Components Library - Reusable UI components for any project",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/saged-ui/saged-ui.esm.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/saged-ui/saged-ui.esm.js",
      "require": "./dist/saged-ui/saged-ui.cjs.js"
    },
    "./angular": {
      "types": "./angular/src/index.d.ts",
      "import": "./angular/src/index.ts"
    },
    "./components": {
      "types": "./dist/types/components.d.ts",
      "import": "./dist/components/index.js"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*.js"
    },
    "./skeleton": {
      "types": "./dist/components/skeleton.d.ts",
      "import": "./dist/components/skeleton.js"
    },
    "./svg-icon": {
      "types": "./dist/components/svg-icon.d.ts",
      "import": "./dist/components/svg-icon.js"
    },
    "./article-editor": {
      "types": "./dist/components/article-editor.d.ts",
      "import": "./dist/components/article-editor.js"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js",
      "require": "./loader/index.cjs"
    },
    "./styles/*": {
      "import": "./dist/styles/*"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Adravilag/saged-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Adravilag/saged-ui/issues"
  },
  "homepage": "https://github.com/Adravilag/saged-ui#readme",
  "files": [
    "dist/",
    "loader/",
    "angular/"
  ],
  "sideEffects": [
    "dist/saged-ui/saged-ui.esm.js",
    "loader/index.js"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test:spec": "stencil test --spec",
    "test:e2e": "stencil test --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "test:coverage": "stencil test --spec --coverage",
    "generate": "stencil generate",
    "storybook": "stencil build && storybook dev -p 6006",
    "build-storybook": "stencil build && storybook build",
    "generate-icons": "node scripts/generate-icons.js",
    "add-icon": "node scripts/generate-icons.js add",
    "icon-server": "node scripts/icon-server.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stencil/angular-output-target": "^1.1.1",
    "@stencil/core": "^4.27.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/html": "^8.6.14",
    "@storybook/html-vite": "^8.6.14",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "prettier": "^3.7.3",
    "puppeteer": "^24.3.0",
    "semantic-release": "^23.1.1",
    "storybook": "^8.6.14",
    "vite": "^6.4.1"
  },
  "keywords": [
    "saged-ui",
    "sagedui",
    "web-components",
    "stencil",
    "design-system",
    "ui-library",
    "ui",
    "components",
    "custom-elements",
    "shadowdom"
  ],
  "author": "adravilag",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
