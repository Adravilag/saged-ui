{
  "name": "saged-ui",
  "version": "1.3.0",
  "description": "SagedUI - Modern Web Components Library - Reusable UI components for any project",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "bin": {
    "saged-ui": "./bin/cli.js"
  },
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/saged-ui/saged-ui.esm.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    },
    "./components": {
      "types": "./dist/types/components.d.ts",
      "import": "./dist/components/index.js"
    },
    "./angular": {
      "types": "./dist/angular/types/saged-ui-angular.d.ts",
      "import": "./dist/angular/fesm2022/saged-ui-angular.mjs"
    },
    "./components/*": {
      "types": "./dist/components/*.d.ts",
      "import": "./dist/components/*"
    },
    "./components/sg-article-editor.js": {
      "types": "./dist/components/sg-article-editor.d.ts",
      "import": "./dist/components/sg-article-editor.js"
    },
    "./components/sg-badge.js": {
      "types": "./dist/components/sg-badge.d.ts",
      "import": "./dist/components/sg-badge.js"
    },
    "./components/sg-button.js": {
      "types": "./dist/components/sg-button.d.ts",
      "import": "./dist/components/sg-button.js"
    },
    "./components/sg-dropdown.js": {
      "types": "./dist/components/sg-dropdown.d.ts",
      "import": "./dist/components/sg-dropdown.js"
    },
    "./components/sg-icon.js": {
      "types": "./dist/components/sg-icon.d.ts",
      "import": "./dist/components/sg-icon.js"
    },
    "./components/sg-skeleton.js": {
      "types": "./dist/components/sg-skeleton.d.ts",
      "import": "./dist/components/sg-skeleton.js"
    },
    "./components/sg-theme-toggle.js": {
      "types": "./dist/components/sg-theme-toggle.d.ts",
      "import": "./dist/components/sg-theme-toggle.js"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js",
      "require": "./loader/index.cjs"
    },
    "./styles/*": {
      "import": "./dist/styles/*"
    },
    "./react": {
      "default": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js"
    },
    "./cli": {
      "require": "./cli/index.js"
    },
    "./cli/plugins/vite": {
      "require": "./cli/plugins/vite.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Adravilag/saged-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Adravilag/saged-ui/issues"
  },
  "homepage": "https://github.com/Adravilag/saged-ui#readme",
  "files": [
    "dist/",
    "loader/",
    "scripts/",
    "bin/",
    "cli/",
    "tools/",
    "react/"
  ],
  "sideEffects": [
    "dist/saged-ui/saged-ui.esm.js",
    "loader/index.js"
  ],
  "scripts": {
    "build": "stencil build",
    "build:all": "turbo run build build:angular build:react",
    "build:angular": "ng-packagr -p angular/ng-package.json",
    "build:react": "cd react && npx tsc -p tsconfig.json",
    "start": "cd docs && npm run dev",
    "dev": "npm run start & npm run docs:dev",
    "test": "turbo run test:spec",
    "test:spec": "stencil test --spec",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:watch": "stencil test --spec --watchAll",
    "test:coverage": "stencil test --spec --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "size": "size-limit",
    "size:check": "size-limit --json",
    "generate": "stencil generate",
    "storybook": "stencil build && storybook dev -p 6006",
    "build-storybook": "stencil build && storybook build",
    "generate-icons": "node scripts/generate-icons.js",
    "add-icon": "node scripts/generate-icons.js add",
    "icon-server": "node scripts/icon-server.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "prepare": "husky",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "stencil build --config stencil.dev.config.ts && cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview"
  },
  "peerDependencies": {
    "@angular/common": ">=18.0.0",
    "@angular/core": ">=18.0.0",
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "@angular/common": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@size-limit/preset-small-lib": "^12.0.0",
    "@stencil/angular-output-target": "^1.1.1",
    "@stencil/core": "^4.27.1",
    "@stencil/react-output-target": "^1.2.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/html": "^8.6.14",
    "@storybook/html-vite": "^8.6.14",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-cli": "^30.2.0",
    "ng-packagr": "^20.0.0",
    "prettier": "^3.7.3",
    "puppeteer": "^24.3.0",
    "semantic-release": "^25.0.2",
    "size-limit": "^12.0.0",
    "storybook": "^8.6.14",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^6.4.1"
  },
  "dependencies": {
    "commander": "^12.1.0"
  },
  "keywords": [
    "saged-ui",
    "sagedui",
    "web-components",
    "stencil",
    "design-system",
    "ui-library",
    "ui",
    "components",
    "custom-elements",
    "shadowdom"
  ],
  "author": "adravilag",
  "license": "MIT",
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/saged-ui/saged-ui.esm.js",
      "limit": "50 KB"
    },
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    }
  ]
}
